				<text><![CDATA[9. METODE PENILAIAN]]></text>
			</staticText>
			<staticText>
				<reportElement x="0" y="41" width="555" height="14" forecolor="#000000" uuid="f67927e5-bb21-4002-914a-137505eee702"/>
